{
  "env": {
    "es2020": true
  },
  "extends": "trybe-fundamentals",
  "rules": {
    "max-lines": "off",
    "max-params": "off",
    "linebreak-style": "off",
    "function-paren-newline": ["error", "never"],
    "no-unused-vars": [
      "error",
      {
        "args": "none",
        "vars": "local",
        "varsIgnorePattern": "data"
      }
    ]
  }
}